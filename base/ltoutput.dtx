%    but for special layouts we might want more control so we provide
%    also separate commands.
%
%    The next command was called \cs{@combinefloats} in the past.
%    \begin{macrocode}
\def \@outputbox@attachfloats {%
    \@outputbox@attachtopfloats
    \@outputbox@attachbottomfloats
}
%    \end{macrocode}
%
%    \begin{macrocode}
\def \@outputbox@attachtopfloats {%
  \ifx \@toplist\@empty \else \@cflt \fi
}
%    \end{macrocode}
%    Before we append the bottom floats we run the
%    \socket{build/column/baselineattach} so that the
%    \cs{textfloatsep} is measured from the baseline if that is desired.
%  \changes{v1.4s}{2025/03/29}{Add socket (footmisc/19)}
%    \begin{macrocode}
\def \@outputbox@attachbottomfloats {%
  \ifx \@botlist\@empty \else
    \@outputbox@append{\UseSocket{build/column/baselineattach}}%
    \@cflb
  \fi
}
%    \end{macrocode}
%  \end{macro}
%  \end{macro}
