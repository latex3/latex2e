%    Now we make all potential input characters active.
% \changes{v0.994}{2000/01/27}{Allow change also in horizontal mode
%      (pr/2888)}
%    \begin{macrocode}
  \@inpenc@loop\^^A\^^H%
  \@inpenc@loop\^^K\^^K%
  \@inpenc@loop\^^N\^^_%
  \@inpenc@loop\^^?\^^ff%
