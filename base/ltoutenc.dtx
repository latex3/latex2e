% \changes{v1.99m}{2016/06/19}{OT1 definition (was duplicate T1 definition)}
%    \begin{macrocode}
\ifx\textcommaabove\@undefined\else
\DeclareTextCompositeCommand{\c}{OT1}{g}{\textcommaabove{g}}
\fi
