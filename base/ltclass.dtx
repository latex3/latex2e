%    \begin{macrocode}
\let\@currpkg@reqd\@empty
%    \end{macrocode}
%
%    \begin{macrocode}
\@onlypreamble\@onefilewithoptions
%    \end{macrocode}
%
%    The kernel no longer uses \cs{@unprocessedoptions}
%    \begin{macrocode}
\let\@unprocessedoptions\@undefined
