  {
    \bool_if:NF \l__fontspec_defining_encoding_bool
      { \__fontspec_error:nn {only-inside-encdef} \EncodingAccent }
    \DeclareTextCommand{#1}{\UnicodeEncodingName}{\add@unicode@accent{#2}}
  }
