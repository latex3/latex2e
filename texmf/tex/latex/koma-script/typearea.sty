        \addtolength{\ta@temp}{\columnsep}%
      \fi
      \setlength{\ta@hblk}{\paperwidth}%
      \addtolength{\ta@hblk}{-\ta@temp}%
      \divide\ta@hblk by3
      \ifdim \ta@hblk <\z@
