  .pdf,.PDF,.eps,.EPS,.mps,.MPS,.ps,.PS,%
  .png,.PNG,.jpg,.JPG,.jpeg,.JPEG,.jp2,.JP2,.jpf,.JPF,.bmp,.BMP,%
