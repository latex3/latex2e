%
%    \begin{macrocode}
%<<*Load patterns in luatex>>
\ifx\directlua\@undefined\else
  \ifx\bbl@luapatterns\@undefined
    \input luababel.def
  \fi
\fi
%<</Load patterns in luatex>>
