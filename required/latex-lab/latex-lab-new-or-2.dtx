%    are in lua mode. If not they get the \plug{default} assigned
%    \begin{macrocode}
\sys_if_engine_luatex:F   
   {         
     \AssignTaggingSocketPlug{build/column/outputbox}{default}
     \AssignTaggingSocketPlug{build/column/footins}{default}
   }
\ExplSyntaxOff
  
